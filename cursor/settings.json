    "matchbooster",
    "matchboosting",
    "millis",
    "MITM",
    "mytable",
    "nclob",
    "nvarchar",
    "odbc",
    "oracledb",
    "patxu",
    "privatelink",
    "reinitializing",
    "resyncing",
    "resyncs",
    "RETRIABLE",
    "rowid",
    "ROWNUM",
    "Schedulable",
    "subquery",
    "TBLPROPERTIES",
